<div id="user-nav">
  <%- if current_user %>
    Logged in as: <b><%= current_user.username %></b>(<%=current_user.score%>) |
    <%= link_to "My account", edit_user_path(current_user) %> |
    <%= link_to "Log out", log_out_path_url, method: :delete %>
  <%- else %>
    <%= link_to "Login in", log_in_path_url %> | <%= link_to "Sign up", sign_up_path_url %>
  <%- end %>
</div>
